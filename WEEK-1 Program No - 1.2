#include<iostream>
using namespace std;
void swap(int *a,int *b)
{
	int temp = *a;
	*a = *b;
	*b = temp;
}
int main()
{
	int a = 385;
	int b = 583;
	cout <<"Before swap: a = " << a << " b = " << b <<endl;
	swap(&a, &b);
	cout <<"After swap: a = "<< a << " b = " << b <<endl;
	return 0;
} 
